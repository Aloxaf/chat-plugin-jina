{
  "$schema": "../node_modules/@lobehub/chat-plugin-sdk/schema.json",
  "api": [
    {
      "name": "websiteReader",
      "url": "https://chat-plugin-jina.aloxaf.com/api/v1",
      "description": "提取网页内容",
      "parameters": {
        "properties": {
          "url": {
            "description": "网页链接",
            "type": "string"
          }
        },
        "required": [
          "url"
        ],
        "type": "object"
      }
    }
  ],
  "author": "Aloxaf",
  "createdAt": "2025-02-28",
  "homepage": "https://github.com/Aloxaf/chat-plugin-jina",
  "identifier": "jina",
  "meta": {
    "avatar": "https://chat-plugin-jina.aloxaf.com/jina.webp",
    "tags": [
      "web",
      "content-crawler"
    ],
    "title": "Jina",
    "description": "Extract content from web links via Jina Reader"
  },
  "settings": {
    "type": "object",
    "required": [
      "JINA_API_KEY"
    ],
    "properties": {
      "JINA_API_KEY": {
        "type": "string",
        "title": "Jina API Key",
        "description": "Your [Jina](https://jina.ai) API Key",
        "format": "password"
      }
    }
  },
  "version": "1"
}