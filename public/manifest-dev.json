{
  "$schema": "../node_modules/@lobehub/chat-plugin-sdk/schema.json",
  "api": [
    {
      "name": "multiWebReader",
      "url": "https://develop.chat-plugin-jina.pages.dev/api/v2/multiWebReader",
      "description": "提取网页内容",
      "parameters": {
        "properties": {
          "urls": {
            "description": "网页链接列表",
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["urls"],
        "type": "object"
      }
    }
  ],
  "author": "Aloxaf",
  "createdAt": "2025-02-28",
  "homepage": "https://github.com/Aloxaf/chat-plugin-jina",
  "identifier": "jina",
  "meta": {
    "avatar": "https://develop.chat-plugin-jina.pages.dev/jina.webp",
    "tags": ["web", "content-crawler"],
    "title": "Jina",
    "description": "Extract content from web links via Jina Reader"
  },
  "settings": {
    "type": "object",
    "required": ["JINA_API_KEY"],
    "properties": {
      "JINA_API_KEY": {
        "type": "string",
        "title": "Jina API Key",
        "description": "Your [Jina](https://jina.ai) API Key",
        "format": "password"
      }
    }
  },
  "version": "1"
}
